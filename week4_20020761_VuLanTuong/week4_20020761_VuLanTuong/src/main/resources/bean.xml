<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="student1" class="org.example.Student">
        <property name="id" value="1"/>
        <property name="name" value="Tuong"/>
        <property name="className" ref="class1"/>
    </bean>

    <bean id="student2" class="org.example.Student">
        <property name="id" value="2"/>
        <property name="name" value="Loc"/>
        <property name="className" ref="class2"/>
    </bean>

    <bean id="class1" class="org.example.Clazz">
        <property name="classId" value="1"/>
        <property name="className" value="KTPM"/>
    </bean>


    <bean id="class2" class="org.example.Clazz">
        <property name="classId" value="2"/>
        <property name="className" value="KHMT"/>
    </bean>

</beans>